{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {},
    "sourceType": "module"
  },
  "rules": {
    "arrow-body-style": 0,
    "consistent-return": 0,
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-console": 0,
    "spaced-comment": 0,
    "func-names": 0,
    "global-require": 0,
    "comma-dangle": 0,
    "quote-props": 0
  }
}
